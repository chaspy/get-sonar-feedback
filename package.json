    "url": "git+https://github.com/chaspy/get-sonar-feedback.git"
  },
  "homepage": "https://github.com/chaspy/get-sonar-feedback#readme",
  "bugs": {
    "url": "https://github.com/chaspy/get-sonar-feedback/issues"
